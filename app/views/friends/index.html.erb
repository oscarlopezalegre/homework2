<%= paginate @users %>

<% @users.each do |i| %>

    <% if i.uid %>
    <%= image_tag "http://graph.facebook.com/"+i.uid.to_s+"/picture?type=square" %>
    <% end %>
	<%= i.name %>
	<br>

	<%= link_to "add as friend" , new_friend_path(i) %>
	
	Friends of session and session<br>	

	<%= 
	Friend.friends(session[:userid],i).present? %>


<% end %>









